<h1>Track Stack</h1>
<% if current_user %>
  <img src="<%= current_user.image %>">
  <%= form_tag destroy_user_session_path, method: :delete do %>
    <button class="btn btn-default">Log out</button>
  <% end %>
<% else %>
  <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
<% end %>

<div id="root"></div>
